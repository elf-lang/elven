elf.printl("clean:")


cleanupexts := "*.pch|*.ilk|*.lib|*.exe|*.exp|*.pdb|*.obj"


// todo: get path list could take a filter?
paths := elf.get_path_list(".", 100)
for path := paths[...] ? {
	if path:match(cleanupexts) ? {
		elf.printl("clean: ", path)
		elf.delete_file(path)
	}
}

elf.printl("clean done")
