xorshift = fun(x) ? {
	x = x ^ x << 13
	x = x ^ x >> 7
	x = x ^ x << 17
	leave x
}

rnd__ = elf.clocktime() // 5282
rnd = fun(min,max) ? {
	if min == max ? leave min
	rnd__ = xorshift(rnd__)
	if rnd__ < 0 ? rnd__ = - rnd__
	leave min + rnd__ % (max - min)
}

rnd_one = fun() ? {
	rnd__ = xorshift(rnd__)
	if rnd__ < 0 ? rnd__ = - rnd__
	leave rnd__ % 10000000000 / 10000000000.
}

rnd_bilateral = fun() ? {
	leave rnd_one() * 2 - 1
}

leave true